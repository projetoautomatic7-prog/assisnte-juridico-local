{
  "name": "assistente-juridico-p",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:with-api": "node scripts/start-dev-with-api.cjs",
    "build": "tsc && vite build",
    "build:backend": "cd backend && npm install && npm run build",
    "build:deploy": "npm install && npm run build && npm run build:backend",
    "start:production": "NODE_ENV=production node -r ./backend/dist/backend/src/datadog.js backend/dist/backend/src/server.js",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=4096 eslint 'src/**/*.{ts,tsx}' 'api/**/*.ts' --ext ts,tsx --report-unused-disable-directives --max-warnings 150 --ignore-pattern 'coverage-api/**' --quiet",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:sarif": "eslint . --ext ts,tsx --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "ensure:sarif-deps": "node -e \"try{require.resolve('@microsoft/eslint-formatter-sarif')}catch{console.log('SARIF formatter not installed, installing...');process.exit(1)}\" || npm install --no-save @microsoft/eslint-formatter-sarif",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test:watch:auto": "vitest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest run --coverage",
    "test:api": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest run --config vitest.api.config.ts",
    "test:integration": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest --config vitest.integration.config.ts",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest",
    "test:run": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest run",
    "test:ui": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitest --ui",
    "test:all": "npm run test:run && npm run test:api && npm run test:integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:chrome": "cd chrome-extension-pje && npm run test",
    "test:e2e:report": "playwright show-report",
    "test:tracing": "pwsh scripts/test-tracing-setup.ps1",
    "eval:run": "node scripts/evaluation/run-agent-evaluation.cjs",
    "eval:analyze": "py scripts/evaluation/evaluate_agents.py",
    "eval:full": "npm run eval:run && npm run eval:analyze",
    "azure:validate": "node scripts/validate-azure-integration.js",
    "azure:deploy-dashboard": "pwsh scripts/deploy-azure-dashboard.ps1",
    "azure:load-test": "az load test-run create --load-test-resource assistente-juridico-load-test --resource-group assistente-juridico-rg --test-id agents-stress-test",
    "sonar": "sonar-scanner"
  },
