
# Configurações da API (Backend)
VITE_API_BASE_URL=http://localhost:3001

# Google Gemini & AI (Obrigatório para os Agentes)
VITE_GOOGLE_API_KEY=sua_chave_api_google_aqui
VITE_GEMINI_API_KEY=sua_chave_api_google_aqui

# Autenticação e Integrações (Google Docs / Login)
VITE_GOOGLE_CLIENT_ID=seu_client_id_google.apps.googleusercontent.com
VITE_GITHUB_OAUTH_CLIENT_ID=seu_client_id_github

# Banco de Dados Vetorial (Qdrant - Para Pesquisa Jurisprudencial)
QDRANT_URL=https://seu-cluster.qdrant.tech
QDRANT_API_KEY=sua_chave_api_qdrant
QDRANT_COLLECTION_NAME=jurisprudencias

# Monitoramento (Opcional)
VITE_SENTRY_DSN=
VITE_OTLP_ENDPOINT=

# Configurações do DJEN (Diário de Justiça Eletrônico)
# Se deixar vazio, usa o fallback via navegador (recomendado para evitar geobloqueio)
DJEN_API_URL=

# Flags de Funcionalidade
VITE_ENABLE_HYBRID_AGENTS=true